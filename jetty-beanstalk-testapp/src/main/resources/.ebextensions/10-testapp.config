container_commands:
#
# remove the override line so test webapp deploys with remote access
#

#
# copy the customize start.ini into place
#
  0001:
    command: cp start.ini /opt/jetty-8
    cwd: /tmp/deployment/application/ROOT/.ebextensions
# 
# copy customized context xml file into place  
#
  0002: 
   command: cp ROOT.xml /opt/jetty-8/contexts
   cwd: /tmp/deployment/application/ROOT/.ebextensions
  0003:
   command: mkdir ROOT.d
   cwd: /opt/jetty-8/contexts
   test: test ! -e ROOT.d
  0004: 
   command: cp override-web.xml /opt/jetty-8/contexts/ROOT.d
   cwd: /tmp/deployment/application/ROOT/.ebextensions
  0005: 
   command: cp javadoc.xml /opt/jetty-8/contexts
   cwd: /tmp/deployment/application/ROOT/.ebextensions